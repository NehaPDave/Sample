    <validationRules>
        <fullName>Check_age</fullName>
        <active>true</active>
        <description>Check age of employee</description>
        <errorConditionFormula>IF (  Age__c  &lt; 21  ||  Age__c &gt; 30 , True , false )</errorConditionFormula>
        <errorDisplayField>Date_of_Birth__c</errorDisplayField>
        <errorMessage>Please provide a valid Date of birth. Employee cannot be less than 21 years of Age and greater than 30 years.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Check_date_of_birth</fullName>
        <active>true</active>
        <description>Date of birth should not be greater than current date.</description>
        <errorConditionFormula>IF( Date_of_Birth__c &gt; Today(), true , false)</errorConditionFormula>
        <errorDisplayField>Date_of_Birth__c</errorDisplayField>
        <errorMessage>Date of birth should not be greater than today&apos;s date.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Last_working_date</fullName>
        <active>true</active>
        <description>Last working date cannot be greater than joining date.</description>
        <errorConditionFormula>IF(Date_of_Joining__c   &gt;=   Last_Working_Day__c  , true , false )</errorConditionFormula>
        <errorDisplayField>Resignation_Date__c</errorDisplayField>
        <errorMessage>Last working date cannot be greater than joining date.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Resignation_date_validation</fullName>
        <active>true</active>
        <description>If notice period is checked to true, resignation date cannot be null.</description>
        <errorConditionFormula>IF ( Is_on_Notice_Period__c  = true &amp;&amp; ISBLANK(Resignation_Date__c) , TRUE ,AND( Is_on_Notice_Period__c == FALSE ,
NOT( ISBLANK( Resignation_Date__c )  ) ) )</errorConditionFormula>
        <errorDisplayField>Resignation_Date__c</errorDisplayField>
        <errorMessage>Please enter a valid registration date. If your are not on notice period , you cannot enter resignation date.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>if_not_active_restrict_salary</fullName>
        <active>true</active>
        <errorConditionFormula>IF ( Is_Active__c == NULL ,  $ObjectType.Salary_Details__c.Fields.Credited_To__c == NULL , FALSE)</errorConditionFormula>
        <errorMessage>cannot create salary record</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>if_not_on_notice_period</fullName>
        <active>true</active>
        <errorConditionFormula>IF(Is_on_Notice_Period__c == FALSE,  Resignation_Date__c == NULL, False)</errorConditionFormula>
        <errorDisplayField>Resignation_Date__c</errorDisplayField>
        <errorMessage>Resignation date cannot be entered as you are not on notice period.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>validate_SSN_number_upto_10_digits</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
    NOT(ISBLANK(SSN__c)),
    MAX(SSN__c) &lt; 10
)</errorConditionFormula>
        <errorDisplayField>SSN__c</errorDisplayField>
        <errorMessage>SSN number should be less than 10.</errorMessage>
    </validationRules>
</CustomObject>
